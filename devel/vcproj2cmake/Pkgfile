# $Id: Pkgtemplate 459 2007-09-24 23:30:12Z root $
#
# Description: Converts Visual Studio project files to CMake
# URL: http://www.eskilson.se
# Maintainer: Roman Senn <roman@digitall.ch>
# Depends on:

name=vcproj2cmake
version=1.0
release=1
source=(http://www.eskilson.se/vcproj2cmake.rb)

build()
{
  install -d $ROOT/bin
  install -m 755 vcproj2cmake.rb $ROOT/bin/vcproj2cmake
}

