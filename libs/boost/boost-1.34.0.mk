# Build targets.
# ---------------------------------------------------------------------------
HEADERS = $(boost_HEADERS)
LIBRARIES = $(boost_LIBRARIES:%=libboost_%.a)
TARGETS = $(LIBRARIES)
DISTFILES = $(call collect,makefiles sources *.ipp)

include $(word 1,$(wildcard $(addsuffix $(MKRULES),./ ../)))

# Installation target
install: all
	
ifneq ($(PROGRAMS),)
	$(INSTALL) -d $(DESTDIR)$(bindir)
	$(INSTALL_EXEC) $(PROGRAMS) $(DESTDIR)$(bindir)
endif
	
ifneq ($(HEADERS),)
	$(foreach DIR,$(sort $(dir $(HEADERS))), \
    $(INSTALL) -d $(DESTDIR)$(includedir)/$(DIR)$(newline) \
    $(INSTALL_DATA) $(wildcard $(DIR)*.hpp $(DIR)*.ipp) $(DESTDIR)$(includedir)/$(DIR)$(newline))
#	for header in $(HEADERS); do \
#    $(INSTALL) -d $(DESTDIR)$(includedir)/$${header%/*}; \
#    $(INSTALL_DATA) $${header} $(DESTDIR)$(includedir)/$${header%/*}; \
#  done
endif
	
ifneq ($(LIBRARIES),)
	$(INSTALL) -d $(DESTDIR)$(libdir)
	$(LIBTOOL) --tag=CXX --mode=install $(INSTALL_DATA) $(LIBRARIES) $(DESTDIR)$(libdir)
endif
	
ifneq ($(IMPORTLIB),)
	$(INSTALL) -d $(DESTDIR)$(libdir)
	$(INSTALL_DATA) $(IMPORTLIB) $(DESTDIR)$(libdir)
endif

