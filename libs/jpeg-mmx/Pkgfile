name=jpeg-mmx
version=0.1.6.tar.gz
release=1
source=($mirror_sf/mjpeg/jpeg-mmx-$version)

build()
{
  cd jpeg-mmx

  ./configure \
       --build="$build" \
       --host="$host" \
       --target="$target" \
       --prefix="$prefix"

  make
  make DESTDIR="$PKG" install
}
