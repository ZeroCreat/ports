name=libunwind
version=0.98.5
release=1
source=(/usr/local/src/distfiles/libunwind-0.98.5.tar.gz)

build()
{
  cd libunwind-$version

  ./configure \
       --build="$build" \
       --host="$host" \
       --target="$target" \
       --prefix="$prefix"

  make
  make DESTDIR="$PKG" install
}
