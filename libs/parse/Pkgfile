# Depends on:

name=parse
version=0.9
release=1
source=(http://www.bluedust.dontexist.com/pub/sources/parse-$version.tar.gz)

build()
{
  cd parse-$version

  ./configure \
       --build="$build" \
       --host="$host" \
       --target="$target" \
       --prefix="$prefix"

  make
  make DESTDIR="$PKG" install
}
