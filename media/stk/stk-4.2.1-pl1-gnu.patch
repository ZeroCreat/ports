Index: stk/autogen.sh
diff -u /dev/null stk/autogen.sh:1.1
--- /dev/null	Fri Jun  2 16:37:03 2006
+++ stk/autogen.sh	Fri Jun  2 16:28:37 2006
@@ -0,0 +1,32 @@
+#!/bin/sh
+(set -x
+ aclocal 2>/dev/null
+ autoconf 
+ libtoolize --automake --copy
+ autoheader
+ automake --add-missing --force-missing --foreign --copy)
+
+sed -i "s,soname_spec='.*',soname_spec='\${name}${release}\$shared_ext'," configure
+
+if [ $(uname -s) = "CYGWIN_NT-5.1" ] || [ $(uname -s) = "WindowsNT" ]; then
+  : ${prefix=c:/dev-cpp}
+  : ${host=i686-pc-mingw32msvc}
+  : ${build=i686-pc-mingw32msvc}
+#  export CXXFLAGS="-g -ggdb -O0"
+else
+  : ${prefix=/usr}
+  : ${host=i686-pc-linux-gnu}
+  : ${build=i686-pc-linux-gnu}
+#  : ${CXXFLAGS="-g -ggdb -O0"}
+fi
+
+: ${CPPFLAGS="-DDEBUG"}
+: ${CC=${prefix}/bin/gcc}
+: ${CXX=${prefix}/bin/g++}
+
+export CPPFLAGS CC CXX
+
+(set -x
+ ./configure --prefix=${prefix} \
+             --host=${host} \
+             --build=${build} "$@")
