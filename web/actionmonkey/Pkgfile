name=actionmonkey
version=20080710
release=1
source=(actionmonkey-$version.tar.bz2)

build()
{
  cd actionmonkey-$version

  ./configure \
       --build="$build" \
       --host="$host" \
       --target="$target" \
       --prefix="$prefix"

  make
  make DESTDIR="$PKG" install
}
